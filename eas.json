{
  "cli": {
    "version": ">= 0.40.0"
  },
  "build": {
    "development": {
      "ios": {
        "resourceClass": "m1-medium"
      },
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_URL": "https://www.pinballmap.com/api/v1"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "androidapk": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "API_URL": "https://www.pinballmap.com/api/v1"
      }
    },
    "production": {
      "distribution": "store",
      "ios": {
        "resourceClass": "m1-medium"
      },
      "env": {
        "API_URL": "https://www.pinballmap.com/api/v1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}